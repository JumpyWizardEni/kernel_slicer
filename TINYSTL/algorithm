#include <vector>

namespace std
{
  template< class T >
  const T min( const T& a, const T& b ) { return a; }

  template< class T >
  const T max( const T& a, const T& b ) { return a; }
  
  template<class X>
  typename vector<X>::iterator back_inserter(vector<X>& vec);

  template< class Iter, class P >
  void copy_if(Iter begin, Iter end, Iter begin2, P predicate) {}
};
