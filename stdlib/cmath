#ifndef STD_CMATH_H
#define STD_CMATH_H

float fmax(float a, float b);
float fmin(float a, float b);

float fmaxf(float a, float b);
float fminf(float a, float b);

float fabs (float x);
float sqrtf(float x);
float sqrt (float x);

float tanf(float x);

static const unsigned int RAND_MAX = 65535;
static const float        MAXFLOAT = 1e37f;

unsigned int rand();

#endif